# run processes and store pids in array
for I in `seq $INTERVAL`; do
	python run_experiment.py -r $P ${FILE}  ${ID}_${I} > ${ID}_${I}.log  &
	pids[${I}]=$!
done

echo ${pids[*]} > pids.log

# wait for all pids
for pid in ${pids[*]}; do
 	wait $pid
done
unset pids
